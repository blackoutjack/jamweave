
aexe(H,_,'#Error',This,Args,HP,This) :-
	length(Args,0),
	aexe(H,_,'#Error',This,'""~&EOL',HP,This).

aexe(H,L,'#Error',This,Args,HP,This) :-
	length(Args,ArgsLen),ArgsLen\=0,
	Args=[V|_],
	ects(H,L,V,H1,_,M),
	heapprop(H1,This,'"message"',M,HP,_).
	
aexe(H,L,'#Error#prototype#toString',This,_,HP,RetVal) :-
	hasprop(This,'"message"',Val),
	ects(H,L,Val,HP,_,RetVal).

