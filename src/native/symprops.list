% This file lists properties that should be modeled as having a symbolic
% value in the initial environment (such as |document.URL|, which
% depends on the URL of the loaded page), and those that have a known
% value that overrides the environment scan.
%
% Format:
% s #ObjectLocation propertyName ValueType [ValuePrototype]
% -- or --
% c #ObjectLocation propertyName value

s #Global defaultStatus String
s #Global defaultstatus String
s #Global devicePixelRatio Number
s #Global frameElement Object
s #Global innerHeight Number
s #Global innerWidth Number
s #Global length Number
s #Global name String
s #Global opener Object #Window#prototype
s #Global outerHeight Number
s #Global outerWidth Number
s #Global pageXOffset Number
s #Global pageYOffset Number
s #Global screenLeft Number
s #Global screenTop Number
s #Global screenX Number
s #Global screenY Number
s #Global scrollX Number
s #Global scrollY Number
s #Global status String
c #Global localStorage #localStorage
s #Global sessionStorage #sessionStorage
% Maybe we can assume the current window is the top.
s #Global top Object #window
c #Global window #Global

s #HTMLDocument#prototype URL String
s #HTMLDocument#prototype alinkColor String
s #HTMLDocument#prototype attributes Object
s #HTMLDocument#prototype baseURI String
s #HTMLDocument#prototype bgColor String
s #HTMLDocument#prototype characterSet String
s #HTMLDocument#prototype charset String
s #HTMLDocument#prototype compatMode String
s #HTMLDocument#prototype cookie String
s #HTMLDocument#prototype defaultCharset String
s #HTMLDocument#prototype designMode String
s #HTMLDocument#prototype dir String
s #HTMLDocument#prototype doctype Object
s #HTMLDocument#prototype documentURI String
s #HTMLDocument#prototype domain String
s #HTMLDocument#prototype fgColor String
s #HTMLDocument#prototype height Number
s #HTMLDocument#prototype inputEncoding String
s #HTMLDocument#prototype lastModified String
s #HTMLDocument#prototype linkColor String

s #Element#prototype scrollHeight Number
s #Element#prototype scrollLeft Number
s #Element#prototype scrollLeftMax Number
s #Element#prototype scrollTop Number
s #Element#prototype scrollTopMax Number
s #Element#prototype scrollWidth Number
s #Element#prototype tagName String
s #Element#prototype textContent String

